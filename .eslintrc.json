{
    "extends": ["airbnb"],
    "plugins": ["prettier"],
    "env": {
      "browser": true,
      "jasmine": true
    },
    "rules": {
      "comma-dangle": 1,
      "no-unused-vars": "warn",
      "no-console": 1,
      "no-unexpected-multiline": "warn"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    }
  }
